{
  "name": "travis-ci-test",
  "version": "1.0.0",
  "description": "A simple test project to implement Continous Integration with TravisCI, Code Climate and Coveralls",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "mocha --exit",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chrismarcel/travis-ci-test.git"
  },
  "keywords": [
    "travis-ci",
    "tdd",
    "mocha",
    "chai",
    "express"
  ],
  "author": "Chrismarcel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Chrismarcel/travis-ci-test/issues"
  },
  "homepage": "https://github.com/Chrismarcel/travis-ci-test#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "express": "^4.16.4",
    "mocha": "^5.2.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0"
  }
}
